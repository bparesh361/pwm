package com.fks.promotion.vo;

import com.fks.promo.common.Resp;

/**
 *
 * @author ajitn
 */
public class CreateProposalResp {

    private Resp resp;
    private String filePath;

    public CreateProposalResp() {
    }

    public CreateProposalResp(Resp resp) {
        this.resp = resp;
    }

    public CreateProposalResp(Resp resp, String filePath) {
        this.resp = resp;
        this.filePath = filePath;
    }

    public String getFilePath() {
        return filePath;
    }

    public void setFilePath(String filePath) {
        this.filePath = filePath;
    }

    public Resp getResp() {
        return resp;
    }

    public void setResp(Resp resp) {
        this.resp = resp;
    }
}
