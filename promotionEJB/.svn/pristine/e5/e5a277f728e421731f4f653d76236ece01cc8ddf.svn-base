package com.eks.ods.article.vo;

/**
 *
 * @author Paresb
 */
public class Resp {
    
    private RespCode code;
    private String msg;

    public Resp(RespCode code, String msg) {
        this.code = code;
        this.msg = msg;
    }

    public RespCode getCode() {
        return code;
    }

    public void setCode(RespCode code) {
        this.code = code;
    }

    public String getMsg() {
        return msg;
    }

    public void setMsg(String msg) {
        this.msg = msg;
    }

    public Resp() {
    }
    
    
    
}
