package com.fks.promotion.vo;

import com.fks.promo.common.Resp;

/**
 *
 * @author krutij
 */
public class GetPromotionReqResponse {

    private Resp resp;
    private PromotionVO promotionVO;

    public GetPromotionReqResponse() {
    }

    public GetPromotionReqResponse(Resp resp) {
        this.resp = resp;
    }

    public GetPromotionReqResponse(Resp resp, PromotionVO promotionVO) {
        this.resp = resp;
        this.promotionVO = promotionVO;
    }

    public PromotionVO getPromotionVO() {
        return promotionVO;
    }

    public void setPromotionVO(PromotionVO promotionVO) {
        this.promotionVO = promotionVO;
    }

    public Resp getResp() {
        return resp;
    }

    public void setResp(Resp resp) {
        this.resp = resp;
    }
    
}
