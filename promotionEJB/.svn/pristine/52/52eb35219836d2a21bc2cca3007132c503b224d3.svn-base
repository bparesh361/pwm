package com.fks.promotion.approval.vo;

import com.fks.promo.common.Resp;

/**
 *
 * @author nehabha
 */
public class PromotionApprRejHoldRes {

    private Resp resp;
    private Long transPromoId;
    private Boolean isLeadTimeFailed;
    private String suggestedTime;

    public PromotionApprRejHoldRes() {
    }

    public PromotionApprRejHoldRes(Resp resp) {
        this.resp = resp;
    }

    public PromotionApprRejHoldRes(Resp resp, Long transPromoId) {
        this.resp = resp;
        this.transPromoId = transPromoId;
    }

    public PromotionApprRejHoldRes(Resp resp, Boolean isLeadTimeFailed) {
        this.resp = resp;
        this.isLeadTimeFailed = isLeadTimeFailed;
    }

    public PromotionApprRejHoldRes(Resp resp, Boolean isLeadTimeFailed, String suggestedTime) {
        this.resp = resp;
        this.isLeadTimeFailed = isLeadTimeFailed;
        this.suggestedTime = suggestedTime;
    }

    public PromotionApprRejHoldRes(Resp resp, Long transPromoId, Boolean isLeadTimeFailed) {
        this.resp = resp;
        this.transPromoId = transPromoId;
        this.isLeadTimeFailed = isLeadTimeFailed;
    }

    public Resp getResp() {
        return resp;
    }

    public void setResp(Resp resp) {
        this.resp = resp;
    }

    public Long getTransPromoId() {
        return transPromoId;
    }

    public void setTransPromoId(Long transPromoId) {
        this.transPromoId = transPromoId;
    }

    public Boolean getIsLeadTimeFailed() {
        return isLeadTimeFailed;
    }

    public void setIsLeadTimeFailed(Boolean isLeadTimeFailed) {
        this.isLeadTimeFailed = isLeadTimeFailed;
    }

    public String getSuggestedTime() {
        return suggestedTime;
    }

    public void setSuggestedTime(String suggestedTime) {
        this.suggestedTime = suggestedTime;
    }
}
