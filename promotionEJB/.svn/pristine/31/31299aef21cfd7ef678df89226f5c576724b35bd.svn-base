package com.fks.promo.master.vo;

/**
 *
 * @author Paresb
 */
public class MstReasonRejectionVO {
    
    private Long reasonId;
    private String reasonName;
    private Short isApprover;
    private Short isBlocked;    

    public MstReasonRejectionVO() {
    }

    public Short getIsBlocked() {
        return isBlocked;
    }

    public void setIsBlocked(Short isBlocked) {
        this.isBlocked = isBlocked;
    }   
    
    public MstReasonRejectionVO(Long reasonId, String reasonName, Short isApprover) {
        this.reasonId = reasonId;
        this.reasonName = reasonName;
        this.isApprover = isApprover;
    }    
    
    public Short getIsApprover() {
        return isApprover;
    }

    public void setIsApprover(Short isApprover) {
        this.isApprover = isApprover;
    }

    public Long getReasonId() {
        return reasonId;
    }

    public void setReasonId(Long reasonId) {
        this.reasonId = reasonId;
    }

    public String getReasonName() {
        return reasonName;
    }

    public void setReasonName(String reasonName) {
        this.reasonName = reasonName;
    }
    
    
    
}
