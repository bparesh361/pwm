package com.fks.promo.master.vo;

/**
 *
 * @author Paresb
 */
public class ProblemMasterVO {
   
    private Long problemId;
    private String problemName;
     private Short isBlocked;

    public Short getIsBlocked() {
        return isBlocked;
    }

    public void setIsBlocked(Short isBlocked) {
        this.isBlocked = isBlocked;
    }
    
    public ProblemMasterVO() {
    }

    public ProblemMasterVO(Long problemId, String problemName) {
        this.problemId = problemId;
        this.problemName = problemName;
    }  
    
    public Long getProblemId() {
        return problemId;
    }

    public void setProblemId(Long problemId) {
        this.problemId = problemId;
    }

    public String getProblemName() {
        return problemName;
    }

    public void setProblemName(String problemName) {
        this.problemName = problemName;
    }
    
    
    
}
