package com.fks.promo.master.vo;

import com.fks.promo.common.Resp;

/**
 *
 * @author Paresb
 */
public class MstMktgVO {  
    
    private Long mktgTyped;
    private String mktgName;
    private Short isBlocked;

    public MstMktgVO(Long mktgTyped, String mktgName) {
        this.mktgTyped = mktgTyped;
        this.mktgName = mktgName;
    }

    public MstMktgVO() {
    }   
    
    public String getMktgName() {
        return mktgName;
    }

    public void setMktgName(String mktgName) {
        this.mktgName = mktgName;
    }

    public Long getMktgTyped() {
        return mktgTyped;
    }

    public void setMktgTyped(Long mktgTyped) {
        this.mktgTyped = mktgTyped;
    }

    public Short getIsBlocked() {
        return isBlocked;
    }

    public void setIsBlocked(Short isBlocked) {
        this.isBlocked = isBlocked;
    }   
    
}
